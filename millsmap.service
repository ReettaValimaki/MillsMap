[Unit]
Description=uWSGI instance to serve millsmap
After=network.target

[Service]
User=reetta
Group=www-data
WorkingDirectory=/home/MillsMapping/millsmap
Environment="PATH=/home/MillsMapping/venv/bin"
ExecStart=/home/MillsMapping/venv/bin/uwsgi --ini millsmap.ini

[Install]
WantedBy=multi-user.target
